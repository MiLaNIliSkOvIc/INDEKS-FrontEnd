{
  "expo": {
    "name": "IndeksFrontEnd",
    "slug": "IndeksFrontEnd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo.png",
    "newArchEnabled": true,
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#538FB1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "38e1ad20-eda9-4cb6-89fc-c66fc5b276c7"
      }
    },
    "owner": "indeks.aplikacija"
  }
}
